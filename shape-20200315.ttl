<https://w3id.org/jp-textbook/CatalogueShape> a sh:NodeShape;
  sh:targetClass <https://w3id.org/jp-textbook/Catalogue>;
  sh:property [
    sh:path <http://schema.org/name>;
    sh:name "書名"@ja;
    sh:name "Title"@en;
    sh:description "言語コードを用いて、書名に加え、そのよみ、英語名称を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>書名（漢字表記）</dd><dt>@ja-Hira</dt><dd>書名（ひらがな表記）。名称のよみをひらがな表記で示します。</dd><dt>@en</dt><dd>英語書名</dd></dl>"@ja;
    sh:description "Using language tags, Japanese name, its transcription (Yomi) and English name are shown. The meanings of the language tags are as follows: <dl><dt>@ja</dt><dd>Japanese title (Kanji)</dd><dt>@ja-Hira</dt><dd>Japanese title (Hiragana)</dd><dt>@en</dt><dd>English title</dd></dl>"@en;
    skos:example "<ul><li>\"高等学校用教科書目録(平成29年度使用)\"@ja</li><li>\"こうとうがっこうようきょうかしょもくろく（へいせい29ねんどしよう）\"@ja-Hira</li><li>\"Textbook Catalogue for Upper Secondary School of 2017 School Year\"@en</li></ul>";
    sh:maxCount 3;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/1999/02/22-rdf-syntax-ns#langString>;
    sh:order 1
  ];
  sh:property [
    sh:path <https://w3id.org/jp-textbook/school>;
    sh:name "学校種別"@ja;
    sh:name "Type of schools"@en;
    skos:example "textbook:school/高等学校";
    sh:maxCount 1;
    sh:minCount 1;
    sh:class <https://w3id.org/jp-textbook/School>;
    sh:order 2
  ];
  sh:property [
    sh:path <http://schema.org/datePublished>;
    sh:name "発行年"@ja;
    sh:name "Publication date"@en;
    skos:example "2016";
    sh:maxCount 1;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#date>;
    sh:order 3
  ];
  sh:property [
    sh:path <https://w3id.org/jp-textbook/usageYear>;
    sh:name "使用年度"@ja;
    sh:name "Usage year"@en;
    sh:description "掲載している教科書の使用年度を表す。"@ja;
    sh:description "Usage year for textbooks listed in the catalogue."@en;
    skos:example "2017";
    sh:maxCount 1;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 4
  ];
  sh:property [
    sh:path <http://schema.org/url>;
    sh:name "本文URL"@ja;
    sh:name "Fulltext URL"@en;
    skos:example <http://www.mext.go.jp/a_menu/shotou/kyoukasho/__icsFiles/afieldfile/2016/04/26/1369944_3.pdf>;
    sh:maxCount 1;
    sh:nodeKind <http://www.w3.org/ns/shacl#IRI>;
    sh:order 5
  ];
  sh:property [
    sh:path <http://www.w3.org/2000/01/rdf-schema#seeAlso>;
    sh:name "関連リンク"@ja;
    sh:name "Related links"@en;
    sh:description "繰り返しあり。本文のアーカイブ（<a href=\"https://archive.org/web/\">Wayback Machine</a>, <a href=\"http://warp.ndl.go.jp/\">WARP</a>など）へのリンク、<a href=\"http://iss.ndl.go.jp\">国立国会図書館サーチ</a>、<a href=\"https://ci.nii.ac.jp/books/\">CiNii Books</a>へのリンク"@ja;
    sh:description "Repeatable. Links to web archiving services (<a href=\"https://archive.org/web/\">Wayback Machine</a> and <a href=\"http://warp.ndl.go.jp/\">WARP</a> etc.), <a href=\"http://iss.ndl.go.jp\">NDL Search</a>, and <a href=\"https://ci.nii.ac.jp/books/\">CiNii Books</a>."@en;
    skos:example "<ul><li>https://web.archive.org/web/*/http://www.mext.go.jp/a_menu/shotou/kyoukasho/__icsFiles/afieldfile/2016/04/26/1369944_3.pdf</li><li>http://id.ndl.go.jp/jpno/000000045845</li><li>https://ci.nii.ac.jp/ncid/AN10586986</li></ul>";
    sh:nodeKind <http://www.w3.org/ns/shacl#IRI>;
    sh:order 6
  ];
  sh:property [
    sh:path <http://dl.nier.go.jp/library/vocab/itemID>;
    sh:name "国立教育政策研究所教育図書館の蔵書データベースにおける所蔵資料ID"@ja;
    sh:name "Item identifier at the NIER Education Library"@en;
    skos:example "400008926";
    sh:maxCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 7
  ];
  sh:property [
    sh:path <http://dl.nier.go.jp/library/vocab/callNumber>;
    sh:name "国立教育政策研究所教育図書館の所蔵資料に対する請求記号"@ja;
    sh:name "Call number at the NIER Education Library"@en;
    sh:description "区切り記号は、「||」である。"@ja;
    sh:description "Delimiter for a call number is \"||\""@en;
    skos:example "375.9||27||2017";
    sh:maxCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 8
  ];
  sh:property [
    sh:path <http://dl.nier.go.jp/library/vocab/recordID>;
    sh:name "国立教育政策研究所教育図書館の蔵書データベースにおけるレコードID"@ja;
    sh:name "Record identifier at the NIER Education Library"@en;
    sh:description "<code>https://nieropac.nier.go.jp/ebopac/<var>レコードID</var></code>の形式で参照可能"@ja;
    sh:description "Link is available as <code>https://nieropac.nier.go.jp/ebopac/<var>record identifier</var></code>."@en;
    skos:example "BB00014905";
    sh:maxCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 9
  ]
.

<https://w3id.org/jp-textbook/CurriculumGuidelineShape> a sh:NodeShape;
  sh:targetClass <https://w3id.org/jp-textbook/CurriculumGuideline>;
  sh:property [
    sh:path <http://schema.org/name>;
    sh:name "名称"@ja;
    sh:name "Name"@en;
    sh:description "言語コードを用いて、名称に加え、そのよみ、英語名称を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>日本語名称（漢字表記）</dd><dt>@ja-Hira</dt><dd>日本語名称（ひらがな表記）。名称のよみをひらがな表記で示します。</dd><dt>@en</dt><dd>英語名称</dd></dl>"@ja;
    sh:description "Using language tags, this property represents an original name as well as transcription and English name. Language codes are as follows:<dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@ja-Hira</dt><dd>Japanese name (Hiragana). Presented a transcription of the name in Hiragana.</dd><dt>@en</dt><dd>English name</dd></dl>"@en;
    skos:example "<ul><li>\"高等学校学習指導要領\"@ja</li><li>\"こうとうがっこうがくしゅうしどうようりょう\"@ja-Hira</li><li>\"Upper Secondary School Curriculum Guideline\"@en</li></ul>";
    sh:maxCount 3;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/1999/02/22-rdf-syntax-ns#langString>;
    sh:languageIn ("ja" "ja-Hira" "en");
    sh:order 1
  ];
  sh:property [
    sh:path <https://w3id.org/jp-textbook/hasSubjectArea>;
    sh:name "教科がある"@ja;
    sh:name "has a subject area"@en;
    skos:example "textbook:curriculum/高等学校/1994/国語";
    sh:minCount 1;
    sh:class <https://w3id.org/jp-textbook/curriculum/SubjectArea>;
    sh:order 2
  ];
  sh:property [
    sh:path <http://schema.org/datePublished>;
    sh:name "告示日"@ja;
    sh:name "Date of announcement"@en;
    skos:example "1989-03-15";
    sh:maxCount 1;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#date>;
    sh:order 3
  ];
  sh:property [
    sh:path <http://schema.org/startDate>;
    sh:name "施行年月日"@ja;
    sh:name "Date of enforcement"@en;
    skos:example "1994-04-01";
    sh:maxCount 1;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#date>;
    sh:order 4
  ];
  sh:property [
    sh:path <https://w3id.org/jp-textbook/school>;
    sh:name "学校種別"@ja;
    sh:name "Type of schools"@en;
    skos:example "textbook:school/高等学校";
    sh:maxCount 1;
    sh:minCount 1;
    sh:class <https://w3id.org/jp-textbook/School>;
    sh:order 5
  ];
  sh:property [
    sh:path <http://schema.org/url>;
    sh:name "本文URL"@ja;
    sh:name "Fulltext URL"@en;
    sh:description "繰り返しあり。本文へのリンク。文部科学省のウェブサイトと<a href=\"http://www.nier.go.jp/guideline/\">国立教育政策研究所学習指導要領データベース</a>へのリンク。"@ja;
    sh:description "Repeatable. Link to the fulltext possibly at the MEXT website and Curriculum guidelines database at NIER"@en;
    skos:example <http://www.mext.go.jp/a_menu/shotou/old-cs/1322503.htm>;
    sh:nodeKind <http://www.w3.org/ns/shacl#IRI>;
    sh:order 6
  ];
  sh:property [
    sh:path <http://purl.org/dc/terms/hasVersion>;
    sh:name "別バージョン情報"@ja;
    sh:name "Other versions"@en;
    sh:description "複数の改正がある場合、改正の件数分、繰り返される。<code>schema:name</code>, <code>schema:datePublished</code>, <code>schema:citation</code>, <code>schema:url</code>, <code>rdfs:seeAlso</code>, <code>schema:isbn</code>, <code>nier:itemID</code>, <code>nier:callNumber</code>, <code>nier:recordID</code>の9つの改正情報用プロパティを持つ。"@ja;
    sh:description "Repeatable. It corresponds to the revisions of the curriculum guideline. A revision information consists of a blank node, which has nine properties <code>schema:name</code>, <code>schema:datePublished</code>, <code>schema:citation</code>, <code>schema:url</code>, <code>rdfs:seeAlso</code>, <code>schema:isbn</code>, <code>nier:itemID</code>, <code>nier:callNumber</code>, and <code>nier:recordID</code>."@en;
    sh:node <https://w3id.org/jp-textbook/CurriculumGuidelineVersionShape>;
    sh:nodeKind <http://www.w3.org/ns/shacl#BlankNode>;
    sh:order 7
  ]
.

<https://w3id.org/jp-textbook/CurriculumGuidelineVersionShape> a sh:NodeShape;
  sh:property [
    sh:path <http://schema.org/name>;
    sh:name "別バージョンの名称"@ja;
    sh:name "Name of the revision"@en;
    sh:description "一部改正であれば、「平成15年12月 一部改正」のような表現となります。また、言語コードを用いて、名称に加え、英語名称を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>日本語名称（漢字表記）</dd><dt>@en</dt><dd>英語名称</dd></dl>"@ja;
    sh:description "If the revision is on a part of a guideline, the name of the revision will be assigned such as \"Partial revision in December 2003\". Using language tags, Japanese and English name are shown. The meanings of language tags are as follows:<dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@en</dt><dd>English name</dd></dl>"@en;
    skos:example "<ul><li>\"平成元年3月 告示\"@ja</li><li>\"1989-03 Notification\"@en</li></ul>";
    sh:minCount 1;
    sh:maxCount 2;
    sh:datatype <http://www.w3.org/1999/02/22-rdf-syntax-ns#langString>;
    sh:languageIn ("ja" "en");
    sh:uniqueLang true;
    sh:order 1
  ];
  sh:property [
    sh:path <http://schema.org/datePublished>;
    sh:name "告示日（改正日）"@ja;
    sh:name "Date of announcement (date of revision)"@en;
    skos:example "1989-03-15";
    sh:minCount 1;
    sh:maxCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#date>;
    sh:order 2
  ];
  sh:property [
    sh:path <http://schema.org/citation>;
    sh:name "出典"@ja;
    sh:name "Reference"@en;
    sh:description "文部科学省（文部省）の告示タイトルおよび掲載された官報についての情報。また、言語コードを用いて、出典に加え、英語の出典を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>日本語出典（漢字表記）</dd><dt>@en</dt><dd>英語出典</dd></dl>"@ja;
    sh:description "Refer to the official announcement and its publication. Using language tags, Japanese and English names shown. The meanings of language tags are as follows: <dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@en</dt><dd>English name</dd></dl>"@en;
    skos:example "<ul><li>\"『平成元年 文部省告示第二十四号』（官報. 平成元年3月15日, 号外特第4号, pp.9-50）\"@ja</li><li>\"\\\"Notification No. 24 in 1989 of the Ministry of Education, Science and Culture\\\" (Official Gazette, 1989-03-15, Special Extra Edition No. 4, pp.9-50)\"@en</li></ul>";
    sh:minCount 1;
    sh:maxCount 2;
    sh:datatype <http://www.w3.org/1999/02/22-rdf-syntax-ns#langString>;
    sh:languageIn ("ja" "en");
    sh:uniqueLang true;
    sh:order 3
  ];
  sh:property [
    sh:path <http://schema.org/url>;
    sh:name "本文URL"@ja;
    sh:name "Fulltext URL"@en;
    sh:description "繰り返しあり。本文へのリンク。文部科学省ウェブページか<a href=\"http://www.nier.go.jp/guideline/\">国立教育政策研究所学習指導要領データベース</a>へのリンク。"@ja;
    sh:description "Repeatable. Links to the fulltext(s), such as MEXT website and <a href=\"http://www.nier.go.jp/guideline/\">NIER Curriculum Guidelines Database</a>."@en;
    skos:example <http://www.mext.go.jp/a_menu/shotou/old-cs/1322235.htm>;
    sh:nodeKind <http://www.w3.org/ns/shacl#IRI>;
    sh:order 4
  ];
  sh:property [
    sh:path <http://www.w3.org/2000/01/rdf-schema#seeAlso>;
    sh:name "関連リンク"@ja;
    sh:name "Related links"@en;
    sh:description "繰り返しあり。<a href=\"http://iss.ndl.go.jp/\">国立国会図書館サーチ</a>、<a href=\"https://ci.nii.ac.jp/books/\">CiNii Books</a>へのリンク。"@ja;
    sh:description "Repeatable. Links to <a href=\"http://iss.ndl.go.jp/\">NDL Search</a> and <a href=\"https://ci.nii.ac.jp/books/\">CiNii Books</a>."@en;
    skos:example <http://id.ndl.go.jp/jpno/89034331>;
    sh:nodeKind <http://www.w3.org/ns/shacl#IRI>;
    sh:order 5
  ];
  sh:property [
    sh:path <http://schema.org/isbn>;
    sh:name "ISBN"@ja;
    sh:name "ISBN"@en;
    skos:example "4171533104";
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 6
  ];
  sh:property [
    sh:path <http://dl.nier.go.jp/library/vocab/itemID>;
    sh:name "国立教育政策研究所教育図書館の蔵書データベースにおける所蔵資料ID"@ja;
    sh:name "Item identifier at the NIER Education Library"@en;
