@prefix schema: <http://schema.org/>.
@prefix textbook: <https://w3id.org/jp-textbook/>.
@prefix curriculum: <https://w3id.org/jp-textbook/curriculum/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix bf: <http://id.loc.gov/ontologies/bibframe/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix nier: <http://dl.nier.go.jp/library/vocab/>.
@prefix textbook-rc: <http://dl.nier.go.jp/library/vocab/textbook-rc/>.
@prefix qb: <http://purl.org/linked-data/cube#>.

textbook:CatalogueShape a sh:NodeShape;
        sh:targetClass textbook:Catalogue;
        sh:property [
          sh:path schema:name;
          sh:order 1;
          sh:minCount 1;
          sh:maxCount 3;
          sh:datatype xsd:string;
          sh:name "書名"@ja;
          sh:name "Title"@en;
          skos:example "<ul><li>\"高等学校用教科書目録(平成29年度使用)\"@ja</li><li>\"こうとうがっこうようきょうかしょもくろく（へいせい29ねんどしよう）\"@ja-Hira</li><li>\"Textbook Catalogue for Upper Secondary School of 2017 School Year\"@en</li></ul>";
          sh:description "言語コードを用いて、書名に加え、そのよみ、英語名称を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>書名（漢字表記）</dd><dt>@ja-Hira</dt><dd>書名（ひらがな表記）。名称のよみをひらがな表記で示します。</dd><dt>@en</dt><dd>英語書名</dd></dl>"@ja;
          sh:description "Using language tags, Japanese name, its transcription (Yomi) and English name are shown. The meanings of the language tags are as follows: <dl><dt>@ja</dt><dd>Japanese title (Kanji)</dd><dt>@ja-Hira</dt><dd>Japanese title (Hiragana)</dd><dt>@en</dt><dd>English title</dd></dl>"@ja;
        ];
        sh:property [
          sh:path textbook:school;
          sh:order 2;
          sh:minCount 1;
          sh:maxCount 1;
          sh:class textbook:School;
          sh:name "学校種別"@ja;
          sh:name "Type of schools"@ja;
          skos:example "textbook:school/高等学校";
        ];
        sh:property [
          sh:path schema:datePublished;
          sh:order 3;
          sh:minCount 1;
          sh:maxCount 1;
          sh:datatype xsd:date;
          sh:name "発行年"@ja;
          sh:name "Publication date"@en;
          skos:example 2016;
        ];
        sh:property [
          sh:path textbook:usageYear;
          sh:order 4;
          sh:minCount 1;
          sh:maxCount 1;
          sh:datatype xsd:string;
          sh:name "使用年度"@ja;
          skos:exmplea 2017; 
          sh:description "掲載している教科書の使用年度を表す。"@ja;
          sh:description "Usage year for textbooks listed in the catalogue."@en;
        ];
        sh:property [
          sh:path schema:url;
          sh:order 5;
          sh:maxCount 1;
          sh:nodeKind sh:IRI;
          sh:name "本文URL"@ja;
          sh:name "Fulltext URL"@ja;
          skos:example "http://www.mext.go.jp/a_menu/shotou/kyoukasho/__icsFiles/afieldfile/2016/04/26/1369944_3.pdf";
        ];
        sh:property [
          sh:path rdfs:seeAlso;
          sh:order 6;
          sh:nodeKind sh:IRI;
          sh:name "関連リンク"@ja;
          sh:name "Related links"@en;
          skos:example "<ul><li>https://web.archive.org/web/*/http://www.mext.go.jp/a_menu/shotou/kyoukasho/__icsFiles/afieldfile/2016/04/26/1369944_3.pdf</li><li>http://id.ndl.go.jp/jpno/000000045845</li><li>https://ci.nii.ac.jp/ncid/AN10586986</li>";
          sh:description "繰り返しあり。本文のアーカイブ（Wayback Machine, WARPなど）へのリンク、国立国会図書館サーチ、CiNii Booksへのリンク"@ja;
          sh:description "Repeatable. Links to web archiving services (Wayback Machine and WARP etc.), NDL Search, and CiNii Books"@en;
        ];
        sh:property [
          sh:path nier:itemID;
          sh:order 7;
          sh:maxCount 1;
          sh:datatype xsd:string;
          sh:name "国立教育政策研究所教育図書館の蔵書データベースにおける所蔵資料ID"@ja;
          sh:name "Item identifier at the NIER Education Library"@en;
          skos:example "400008926";
        ];
        sh:property [
          sh:path nier:callNumber;
          sh:order 8;
          sh:maxCount 1;
          sh:datatype xsd:string;
          sh:name "国立教育政策研究所教育図書館の所蔵資料に対する請求記号"@ja;
          sh:name "Call number at the NIER Education Library"@en;
          skos:example "375.9||27||2017";
          sh:description "区切り記号は、「||」である。"@ja;
          sh:description "Delimiter for a call number is \"||\""@en;
        ];
        sh:property [
          sh:path nier:recordID;
          sh:order 9;
          sh:maxCount 1;
          sh:datatype xsd:string;
          sh:name "国立教育政策研究所教育図書館の蔵書データベースにおけるレコードID"@ja;
          sh:name "Record identifier at the NIER Education Library"@en;
          skos:example "BB00014905";
          sh:description "<code>https://nieropac.nier.go.jp/ebopac/<var>レコードID</var></code>の形式で参照可能"@ja;
          sh:description "Link is available as <code>https://nieropac.nier.go.jp/ebopac/<var>record identifier</var></code>."@en;
        ];
.

textbook:CurriculumGuidelineShape a sh:NodeShape;
        sh:targetClass textbook:CurriculumGuideline;
        sh:property [
          sh:path schema:name;
          sh:order 1;
          sh:minCount 1;
          sh:maxCount 1;
          sh:datatype xsd:string;
          sh:name "名称"@ja;
          sh:name "Name"@en;
          skos:example "<ul><li>\"高等学校学習指導要領\"@ja</li><li>\"こうとうがっこうがくしゅうしどうようりょう\"@ja-Hira</li><li>\"Upper Secondary School Curriculum Guideline\"@en</li></ul>";
          sh:description "言語コードを用いて、名称に加え、そのよみ、英語名称を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>日本語名称（漢字表記）</dd><dt>@ja-Hira</dt><dd>日本語名称（ひらがな表記）。名称のよみをひらがな表記で示します。</dd><dt>@en</dt><dd>英語名称</dd></dl>"@ja;
          sh:description "Using language tags, this property represents an original name as well as transcription and English name. Language codes are as follows:<dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@ja-Hira</dt><dd>Japanese name (Hiragana). Presented a transcription of the name in Hiragana.</dd><dt>@en</dt><dd>English name</dd></dl>"@en;
        ];
        sh:property [
          sh:path schema:datePublished;
          sh:order 2;
          sh:minCount 1;
          sh:maxCount 1;
          sh:datatype xsd:date;
          sh:name "告示日"@ja;
          sh:name "Date of announcement"@en;
          skos:example "1989-03-15";
        ];
        sh:property [
          sh:path schema:startDate;
          sh:order 3;
          sh:minCount 1;
          sh:maxCount 1;
          sh:datatype xsd:date;
          sh:name "施行年月日"@ja;
          sh:name "Date of enforcement"@en;
          skos:example "1994-04-01";
        ];
        sh:property [
          sh:path schema:url;
          sh:order 4;
          sh:nodeKind sh:IRI;
          sh:name "本文URL"@ja;
          sh:name "Fulltext URL"@en;
          skos:example "http://www.mext.go.jp/a_menu/shotou/old-cs/1322503.htm";
          sh:description "繰り返しあり。本文へのリンク。文部科学省のウェブサイトと国立教育政策研究所学習指導要領データベースへのリンク。"@ja;
          sh:description "Repeatable. Link to the fulltext possibly at the MEXT website and Curriculum guidelines database at NIER"@en;
        ];
        sh:property [
          sh:path textbook:hasSubjectArea;
          sh:order 5;
          sh:minCount 1;
          sh:class curriculum:SubjectArea;
          sh:name "教科がある"@ja;
          sh:name "has a subject area"@en;
          skos:example "textbook:curriculum/高等学校/1994/国語";
        ];
        sh:property [
          sh:path textbook:school;
          sh:order 6;
          sh:minCount 1;
          sh:maxCount 1;
          sh:class textbook:School;
          sh:name "学校種別"@ja;
          sh:name "Type of schools"@en;
          skos:example "textbook:school/高等学校";
        ];
        sh:property [
          sh:path dct:hasVersion;
          sh:order 7;
          sh:nodeKind sh:BlankNode;
          sh:name "別バージョン情報"@ja;
          sh:name "Other versions"@en;
          sh:description "複数の改正がある場合、改正の件数分、繰り返される。<code>schema:name</code>, <code>schema:datePublished</code>, <code>schema:citation</code>, <code>schema:url</code>, <code>rdfs:seeAlso</code>, <code>schema:isbn</code>, <code>nier:itemID</code>, <code>nier:callNumber</code>, <code>nier:recordID</code>の9つの改正情報用プロパティを持つ。"@ja;
          sh:description "Repeatable. It corresponds to the revisions of the curriculum guideline. A revision information consists of a blank node, which has nine properties <code>schema:name</code>, <code>schema:datePublished</code>, <code>schema:citation</code>, <code>schema:url</code>, <code>rdfs:seeAlso</code>, <code>schema:isbn</code>, <code>nier:itemID</code>, <code>nier:callNumber</code>, and <code>nier:recordID</code>."@en;
          sh:node [
            sh:property [
              sh:path schema:name;
              sh:order 1;
              sh:minCount 1;
              sh:maxCount 2;
              sh:datatype xsd:string;
              sh:uniqueLang true;
              sh:languageIn ("ja" "en");
              sh:name "別バージョンの名称"@ja;
              sh:name "Name of the revision"@en;
              skos:example "<ul><li>\"平成元年3月 告示\"@ja</li><li>\"1989-03 Notification\"@en</li></ul>";
              sh:description "一部改正であれば、「平成15年12月 一部改正」のような表現となります。また、言語コードを用いて、名称に加え、英語名称を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>日本語名称（漢字表記）</dd><dt>@en</dt><dd>英語名称</dd></dl>"@ja;
              sh:description "If the revision is on a part of a guideline, the name of the revision will be assigned such as \"Partial revision in December 2003\". Using language tags, Japanese and English name are shown. The meanings of language tags are as follows:<dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@en</dt><dd>English name</dd></dl>"@ja;
            ];
            sh:property [
              sh:path schema:datePublished;
              sh:order 2;
              sh:minCount 1;
              sh:maxCount 1;
              sh:datatype xsd:string;
              sh:name "告示日（改正日）"@ja;
              sh:name "Date of announcement (date of revision)"@en;
              skos:example "1989-03-15";
            ];
            sh:property [
              sh:path schema:citation;
              sh:order 3;
              sh:minCount 1;
              sh:maxCount 2;
              sh:datatype xsd:string;
              sh:uniqueLang true;
              sh:languageIn ("ja" "en");
              sh:name "出典"@ja;
              sh:name "Reference"@en;
              skos:example "<ul><li>\"『平成元年 文部省告示第二十四号』（官報. 平成元年3月15日, 号外特第4号, pp.9-50）\"@ja</li><li>\"\\\"Notification No. 24 in 1989 of the Ministry of Education, Science and Culture\\\" (Official Gazette, 1989-03-15, Special Extra Edition No. 4, pp.9-50)\"@en</li></ul>";
              sh:description "文部科学省（文部省）の告示タイトルおよび掲載された官報についての情報。また、言語コードを用いて、出典に加え、英語の出典を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>日本語出典（漢字表記）</dd><dt>@en</dt><dd>英語出典</dd></dl>"@ja;
              sh:description "Refer to the official announcement and its publication. Using language tags, Japanese and English names shown. The meanings of language tags are as follows: <dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@en</dt><dd>English name</dd></dl>"@en;
            ];
            sh:property [
              sh:path schema:url;
              sh:order 4;
              sh:nodeKind sh:IRI;
              sh:name "本文URL"@ja;
              sh:name "Fulltext URL"@en;
              skos:example "http://www.mext.go.jp/a_menu/shotou/old-cs/1322235.htm";
              sh:description "繰り返しあり。本文へのリンク。文部科学省ウェブページか国立教育政策研究所学習指導要領データベースへのリンク。"@ja;
              sh:description "Repeatable. Links to the fulltext(s), such as MEXT website and NIER Curriculum Guidelines Database."@en;
            ];
            sh:property [
              sh:path rdfs:seeAlso;
              sh:order 5;
              sh:nodeKind sh:IRI;
              sh:name "関連リンク"@ja;
              sh:name "Related links"@en;
              skos:example "http://id.ndl.go.jp/jpno/89034331";
              sh:description "繰り返しあり。国立国会図書館サーチ、CiNii Booksへのリンク。"@ja;
              sh:description "Repeatable. Links to NDL Search and CiNii Books."@en;
            ];
            sh:property [
              sh:path schema:isbn;
              sh:order 6;
              sh:datatype xsd:string;
              sh:name "ISBN"@ja;
              sh:name "ISBN"@en;
              skos:example "4171533104";
            ];
            sh:property [
              sh:path nier:itemID;
              sh:order 6;
              sh:maxCount 1;
              sh:datatype xsd:string;
              sh:name "国立教育政策研究所教育図書館の蔵書データベースにおける所蔵資料ID"@ja;
              sh:name "Item identifier at the NIER Education Library"@en;
              skos:example "071221843";
            ];
            sh:property [
              sh:path nier:callNumber;
              sh:order 6;
              sh:maxCount 1;
              sh:datatype xsd:string;
              sh:name "国立教育政策研究所教育図書館の所蔵資料に対する請求記号"@ja;
              sh:name "Call number at the NIER Education Library"@en;
              skos:example "375.92||1||1989d";
              sh:description "区切り記号は、「||」である。"@ja;
              sh:description "Delimiter for a call number is \"||\""@en;
            ];
            sh:property [
              sh:path nier:recordID;
              sh:order 7;
              sh:maxCount 1;
              sh:datatype xsd:string;
              sh:name "国立教育政策研究所教育図書館の蔵書データベースにおけるレコードID"@ja;
              sh:name "Record identifier at the NIER Education Library"@en;
              skos:example "BB80025218";
              sh:description "<code>https://nieropac.nier.go.jp/ebopac/<var>レコードID</var></code>の形式で参照可能"@ja;
              sh:description "Link is available as <code>https://nieropac.nier.go.jp/ebopac/<var>record identifier</var></code>."@en;
            ];
          ];
        ];
.

textbook:PublisherShape a sh:NodeShape;
        sh:targetClass textbook:Publisher;
        sh:property [
          sh:path schema:name;
          sh:order 1;
          sh:minCount 1;
          sh:maxCount 3;
          sh:datatype xsd:string;
          sh:uniqueLang true;
          sh:languageIn ("ja" "ja-Hira" "ja-Latn");
          sh:name "名称"@ja;
          sh:name "Name"@en;
          skos:example "<ul><li>\"音楽之友社\"@ja;</li><li>\"おんがく の ともしゃ\"@ja-Hira;</li><li>\"Ongaku No Tomosha\"@ja-Latn;</li></ul>";
          sh:description "言語コードを用いて、名称に加え、そのよみ、よみのローマ字表記を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>名称（漢字表記）</dd><dt>@ja-Hira</dt><dd>名称（ひらがな表記）。名称のよみをひらがな表記で示します。</dd><dt>@ja-Latn</dt><dd>名称（ローマ字表記）。名称のよみをローマ字表記で示します。</dd>"@ja;
          sh:description "Using language tags, the name and its transcription (Yomi) in Hiragana and in Alphabetical form. The meanings of language tags are as follows:<dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@ja-Hira</dt><dd>Japanese name in Hiragana.</dd><dt>@ja-Latn</dt><dd>Japanese name in an alphabetical form.</dd>"@en;
        ];
        sh:property [
          sh:path rdfs:seeAlso;
          sh:order 2;
          sh:nodeKind sh:IRI;
          sh:name "関連リンク"@ja;
          sh:name "Related links"@en;
          skos:example "<https://id.ndl.go.jp/auth/ndlna/00258007>";
          sh:description "繰り返しあり。<a href=\"https://hojin-info.go.jp/hojin/TopPage\">法人インフォ</a>、<a href=\"https://id.ndl.go.jp/auth/ndla\">Web NDL Authorities</a>、<a href=\"http://ja.dbpedia.org/\">DBpedia</a>へのリンク"@ja;
          sh:description "Repeatable. Links to <a href=\"https://hojin-info.go.jp/hojin/TopPage\">Houjin Info</a>, <a href=\"https://id.ndl.go.jp/auth/ndla\">Web NDL Authorities</a> and <a href=\"http://ja.dbpedia.org/\">DBpedia</a>."@en;
        ];
        sh:property [
          sh:path textbook:catalogueYear;
          sh:order 3;
          sh:minCount 1;
          sh:maxCount 1;
          sh:datatype xsd:date;
          sh:name "掲載教科書目録の発行年"@ja;
          sh:name "Publication year for a textbook catalogue where the publisher is listed"@en;
          skos:example "2017";
        ];
        sh:property [
          sh:path textbook:catalogue;
          sh:order 4;
          sh:minCount 1;
          sh:class textbook:Catalogue;
          sh:name "掲載教科書目録"@ja;
          sh:name "Textbook catalogue listed"@ja;
          skos:example "textbook:catalogue/高等学校/2017";
          sh:description "繰り返しあり。<code>textbook:catalogue/<var>学校種別</var>/<var>教科書目録発行年</var></code>として示される教科書目録リソースと関連付ける"@ja;
          sh:description "Repeatable. Add link to the textbook catalogue resource represented as <code>textbook:catalogue/<var>type of schools</var>/<var>catalogue publication year</var></code>"@en;
        ];
        sh:property [
          sh:path textbook:publisherAbbreviation;
          sh:order 5;
          sh:minCount 1;
          sh:maxCount 1;
          sh:datatype xsd:string;
          sh:name "発行者略称"@ja;
          sh:name "Abbreviation for the publisher"@en;
          skos:example "友社";
        ];
        sh:property [
          sh:path textbook:publisherNumber;
          sh:order 6;
          sh:minCount 1;
          sh:datatype xsd:integer;
          sh:name "発行者番号"@ja;
          sh:name "Publisher number"@en;
          skos:example "89";
          sh:description "繰り返しあり。（例：<a href=\"https://w3id.org/jp-textbook/publisher/2009/日文\">textbook:publisher/2009/日文</a>）"@ja;
          sh:description "Repeatable. ex. <a href=\"https://w3id.org/jp-textbook/publisher/2009/日文\">textbook:publisher/2009/日文</a>."@en;
        ];
        sh:property [
          sh:path bf:note;
          sh:order 7;
          sh:maxCount 1;
          sh:datatype xsd:string;
          sh:name "注記"@ja;
          sh:name "Notes"@en;
          skos:example "発行者番号が2種類ある（大阪書籍の教科書は発行者番号：3）。";
        ];
.

textbook:SchoolShape a sh:NodeShape;
        sh:targetClass textbook:School;
        sh:property [
          sh:path schema:name;
          sh:order 1;
          sh:minCount 1;
          sh:maxCount 3;
          sh:uniquLang true;
          sh:languagesIn ("ja" "ja-Hira" "en");
          sh:name "名称"@ja;
          sh:name "Name"@en;
          skos:example "<ul><li>\"小学校\"@ja;</li><li>\"しょうがっこう\"@ja-Hira;</li><li>\"Elementary School\"@en;</li></ul>";
          sh:description "言語コードを用いて、名称に加え、そのよみ、英語名称を示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>名称（漢字表記）</dd><dt>@ja-Hira</dt><dd>名称（ひらがな表記）。名称のよみをひらがな表記で示します。</dd><dt>@en</dt><dd>英語名称</dd></dl>"@ja;
          sh:description "Using language tags, Japanese name and its transcription and English name are shown. The meanings of language tags are as follows:<dl><dt>@ja</dt><dd>Japanese name (Kanji)</dd><dt>@ja-Hira</dt><dd>Japanese name (Hiragana)</dd><dt>@en</dt><dd>English name</dd></dl>"@en;
        ];
        sh:property [
          sh:path owl:sameAs;
          sh:order 2;
          sh:minCount 1;
          sh:maxCount 1;
          sh:nodeKind sh:IRI;
          sh:name "同一リソース"@ja;
          sh:name "Equivalent resource"@en;
          skos:example "<http://ja.dbpedia.org/resource/特別支援学校>";
        ];
.

textbook:SubjectTypeShape a sh:NodeShape;
        sh:targetClass textbook:SubjectType;
        sh:property [
          sh:path schema:name;
          sh:order 1;
          sh:minCount 1;
          sh:maxCount 2;
          sh:datatype xsd:string;
          sh:uniqueLang true;
          sh:languageIn ("ja" "ja-Hira");
          sh:name "名称"@ja;
          sh:name "Name"@en;
          skos:example "<ul><li>\"各学科に共通する各教科\"@ja;</li><li>\"かくがっかにきょうつうするかくきょうか\"@ja-Hira;</li></ul>";
          sh:description "言語コードを用いて、名称に加え、そのよみを示します。言語コードとその意味は以下の通りです。<dl><dt>@ja</dt><dd>名称（漢字表記）</dd><dt>@ja-Hira</dt><dd>名称（ひらがな表記）。名称のよみをひらがな表記で示します。</dd></dl>"@ja;
        ];
        sh:property [
          sh:path textbook:school;
          sh:order 2;
          sh:minCount 1;
          sh:maxCount 1;
          sh:class textbook:School;
          sh:name "学校種別"@ja;
          sh:name "Type of schools"@en;
          skos:example "textbook:school/高等学校";
        ];
        sh:property [
