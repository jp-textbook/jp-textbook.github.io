          sh:minCount 1;
          sh:maxCount 1;
          sh:class textbook:CurriculumGuideline;
          sh:name "対応する学習指導要領"@ja;
          sh:name "Curriculum Guideline"@en;
          skos:example "textbook:curriculum/中学校/2002";
          sh:description "<code>textbook:curriculum/<var>学校種別</var>/<var>学習指導要領施行年</var></code>として示される学習指導要領リソースと関連付ける"@ja;
          sh:description "Link to the curriculum guideline resource, represented as <code>textbook:curriculum/<var>type of school</var>/<var>year of curriculum guideline</var></code>."@en;
