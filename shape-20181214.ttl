          sh:description "Link to the subject resource, represented as <code>textbook:curriculum/<var>type of school</var>/<var>year of curriculum guideline</var>/<var>subject area</var>/<var>subject</var></code>."@en;
        ];
        sh:property [
          sh:path textbook:subjectArea;
          sh:order 17;
          sh:minCount 1;
          sh:maxCount 1;
          sh:class curriculum:SubjectArea;
          sh:name "教科"@ja;
          sh:name "Subject area"@en;
          skos:example "textbook:curriculum/中学校/2002/外国語";
          sh:description "<code>textbook:curriculum/<var>学校種別</var>/<var>学習指導要領施行年</var>/<var>教科</var></code>として示される科目リソースと関連付ける"@ja;
          sh:description "Link to the subject area resource, represented as <code>textbook:curriculum/<var>type of school</var>/<var>year of curriculum guideline</var>/<var>subject area</var></code>."@en;
